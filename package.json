{
  "name": "jsonschema-key-compression",
  "version": "1.0.1",
  "description": "Compress json-data based on it's json-schema",
  "author": "pubkey",
  "keywords": [
    "json",
    "compression",
    "jsonschema"
  ],
  "main": "./dist/es5/index.js",
  "jsnext:main": "./dist/es6/index.js",
  "module": "./dist/es6/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubkey/jsonschema-key-compression.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pubkey/jsonschema-key-compression/issues"
  },
  "homepage": "https://github.com/pubkey/jsonschema-key-compression#readme",
  "scripts": {
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "transpile": "tsc -p ./ && echo '# transpile es5 sucess!'",
    "transpile:es6": "tsc -p ./ --target ES6 --outDir ./dist/es6 && echo '# transpile es6 sucess!'",
    "build": "rimraf -r ./dist && npm run transpile && npm run transpile:es6",
    "test": "mocha -r ts-node/register test/unit/index.test.ts",
    "test:performance": "mocha -r ts-node/register --bail --exit ./test/performance.test.ts",
    "dev": "mocha -r ts-node/register test/unit/index.test.ts  --watch --watch-extensions ts"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "assert": "2.0.0",
    "async-test-util": "1.7.3",
    "mocha": "6.1.4",
    "rimraf": "2.6.3",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "dependencies": {}
}
